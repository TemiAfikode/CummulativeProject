@model IEnumerable<CummulativeProject.Models.Teacher>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "New")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Teacherfname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Teacherlname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employeenumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HireDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Teacherfname)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Teacherlname)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Employeenumber)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HireDate)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Salary)
    </td>
    <td>
        @Html.ActionLink("Details", "Show", new { id = item.TeacherId }) |
        @Html.ActionLink("Delete", "DeleteConfirm", new { id = item.TeacherId })
    </td>
</tr>
}

</table>
